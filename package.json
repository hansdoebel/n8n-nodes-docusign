{
	"name": "n8n-nodes-docusign",
	"version": "0.0.2",
	"description": "Docusign custom node for n8n",
	"keywords": [
		"n8n-community-node-package"
	],
	"license": "MIT",
	"homepage": "https://github.com/hansdoebel/n8n-nodes-docusign#readme",
	"author": {
		"name": "Hans Doebel",
		"email": "hansdev@mailbox.org"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/hansdoebel/n8n-nodes-docusign.git"
	},
	"bugs": {
		"url": "https://github.com/hansdoebel/n8n-nodes-docusign/issues"
	},
	"engines": {
		"node": ">=18.10"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "tsc && tsc-alias && bun run build-icons.ts",
		"dev": "n8n-node dev",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials package.json",
		"lintfix": "eslint nodes credentials package.json --fix",
		"test": "bun test",
		"test:watch": "bun test --watch",
		"test:auth": "bun test tests/auth.test.ts",
		"test:v2": "bun test tests/v2-*.test.ts",
		"prepublishOnly": "bun run build && bun run lint -c .eslintrc.prepublish.js nodes credentials package.json"
	},
	"files": [
		"dist"
	],
	"funding": {
		"type": "buymeacoffee",
		"url": "https://buymeacoffee.com/hansdoebel"
	},
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/DocusignOAuth2Api.credentials.js"
		],
		"nodes": [
			"dist/nodes/Docusign/Docusign.node.js",
			"dist/nodes/Docusign/DocusignTrigger.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "^0.17.0",
		"@types/bun": "latest",
		"@types/node": "^22.19.1",
		"@typescript-eslint/parser": "^7.18.0",
		"eslint": "^8.57.1",
		"eslint-plugin-n8n-nodes-base": "^1.16.4",
		"prettier": "^3.7.4",
		"tsc-alias": "^1.8.16",
		"typescript": "^5.9.3"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
